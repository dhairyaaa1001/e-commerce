                                                <th scope="col"><%=admindata[i].adminEmail%></th>
                                                <th scope="col"><%=admindata[i].adminPassword%></th>
                                                <th scope="col"><%=admindata[i].adminAge%></th>
                                                <th><form action="/admin/delete/<%=admindata[i]._id%>" method="post">
                                                    <button type="submit" class="btn btn-danger btn-sm" onclick="return deleteEntry();">Delete</button>
