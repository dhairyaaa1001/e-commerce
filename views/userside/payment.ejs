    <script type="text/javascript">
      $(function () {
        $("input[name='paymentoption']").click(function () {
          if ($("#pcard").is(":checked")) {
            $("#txt1").show();
            $("#txt2").show();
            $("#txt3").show();
            $("#upitxt").hide();
            $("#upiimg").hide();
          } else if ($("#pupi").is(":checked")) {
    
            $("#txt1").hide();
            $("#txt2").hide();
            $("#txt3").hide();
            $("#upitxt").show();
            $("#upiimg").show();
          } else {
            $("#txt1").hide();
            $("#txt2").hide();
            $("#txt3").hide();
            $("#upitxt").hide();
            $("#upiimg").hide();
          }
        });
      });
    
      $(document).ready(function () {
        $("#txt1").hide();
        $("#txt2").hide();
        $("#txt3").hide();
        $("#upitxt").hide();
        $("#upiimg").hide();
      });
    
      $(".uip_id").hide();
